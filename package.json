{
  "name": "todo-list-challenge",
  "version": "1.0.0",
  "description": "To-Do List Challenge",
  "main": "src/index.js",
  "scripts": {
    "start": "npm run dev",
    "dev": "NODE_ENV=development webpack-dev-server --env development --config webpack/develop.js --inline --progress --profile --colors --watch --display-error-details --display-cached",
    "build": "NODE_ENV=production webpack --env production --config webpack/production.js --progress",
    "build:run": "cd dist/ && python3 -m http.server 3000 && cd ..",
    "test": "NODE_ENV=test jest --notify --colors --config jest/config.js --no-cache -u",
    "test:watch": "NODE_ENV=test jest --colors --config jest/config.js --no-cache --watch -u",
    "test:coverage": "NODE_ENV=test jest --config jest/config.js --coverage -u",
    "test:coverage:watch": "NODE_ENV=test jest --colors --config jest/config.js --watch --coverage -u"
  },
  "author": "Luca Anceschi <luca.anceschi@gmail.com>",
  "license": "ISC",
