    "react-dom": "^16.13.1",
    "react-hook-form": "^6.0.8",
    "react-redux": "^7.2.0",
    "react-router-dom": "^5.2.0",
    "redux": "^4.0.5",
    "redux-saga": "^1.1.3",
    "uuid": "^8.2.0"
  },
  "devDependencies": {
