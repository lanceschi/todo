    "@testing-library/react": "^10.4.7",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.24",
    "build-number-generator": "0.0.6",
    "css-loader": "^3.6.0",
    "eslint": "^7.5.0",
    "file-loader": "^6.0.0",
